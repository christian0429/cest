    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "framer-motion": "10.16.4",
    "history": "5.0.0",
    "husky": "4.2.5",
    "parcel": "2.8.3",
    "prettier": "3.0.3",
    "react": "18.2.0",
    "react-aria": "3.25.0",
    "react-dom": "18.2.0",
    "react-icons": "4.2.0",
    "react-router": "6.0.0-beta.0",
