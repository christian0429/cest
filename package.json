    "format:check": "prettier . --check",
    "format:write": "prettier . --write",
    "lint": "eslint src/* --fix --max-warnings=0",
    "link:self": "yalc publish && cd example && yalc add react-modal-sheet",
    "test": "echo \"Todo fix tests...\"",
